	reset-gpios = <0x66 0x1b 0x01>;

	display-timings {
		native-mode = <0xa0>;

		timing0 {
			clock-frequency = <0x1c9c380>;
			hactive = <0x280>;
			vactive = <0x1e0>;
			hfront-porch = <0x2e>;
			hsync-len = <0x02>;
			hback-porch = <0x2c>;
			vfront-porch = <0x10>;
			vsync-len = <0x02>;
			vback-porch = <0x0e>;
			hsync-active = <0x00>;
			vsync-active = <0x00>;
			de-active = <0x00>;
			pixelclk-active = <0x00>;
			phandle = <0xa0>;
		};
	};

	ports {
		#address-cells = <0x01>;
		#size-cells = <0x00>;

		port@0 {
			reg = <0x00>;

			endpoint {
				remote-endpoint = <0xa1>;
				phandle = <0x9d>;
			};
		};
	};
};

  timing0 = "/dsi@ff450000/panel@0/display-timings/timing0";

  panel_in_dsi = "/dsi@ff450000/panel@0/ports/port@0/endpoint";

  rkisp1 = "/rkisp1@ff4a0000";

  isp0_mipi_in = "/rkisp1@ff4a0000/port/endpoint@0";

  backlight = "/backlight";